<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:EventPlannerMAUI.MVVM.View"
            x:Class="EventPlannerMAUI.MVVM.View.HomeNavigationPage"
            IsGestureEnabled="False" >

    <FlyoutPage.Flyout>

        <local:MainFlyoutMenuPage
            x:Name="MainFlyoutPage" />

    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>

        <NavigationPage>

            <x:Arguments>

                <local:EventListPage />

            </x:Arguments>

        </NavigationPage>

    </FlyoutPage.Detail>

</FlyoutPage>
